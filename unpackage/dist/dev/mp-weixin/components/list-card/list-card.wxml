<view data-event-opts="{{[['tap',[['open',['$event']]]]]}}" bindtap="__e"><view class="listcard"><view class="listcard-image"><image src="{{''+item.cover_img}}" mode="aspectFill"></image></view><view class="listcard-content"><view class="listcard-content_title"><text>{{item.title}}</text><block wx:if="{{!download}}"><collect vue-id="a311777c-1" item="{{item}}" collect="{{collect}}" bind:__l="__l"></collect></block></view><view class="listcard-content_des"><block wx:if="{{!download}}"><view class="listcard-content_des-label"><view class="listcard-content_des-label-item">{{item.label}}</view></view></block><block wx:if="{{!download}}"><view class="listcard-content_des-info"><view class="listcard-content_des-info-item">{{''+item.created_at+''}}</view><view class="listcard-content_des-info-item">{{''+item.download_num+'次下载'}}</view><block wx:if="{{item.price>0}}"><view class="listcard-content_des-info-item">{{''+item.price+'P豆'}}</view></block><block wx:else><view class="listcard-content_des-info-item free">免费</view></block></view></block><block wx:else><view class="down-info-box"><view class="listcard-content_des-info-item">{{'支付：'+item.price+' P豆'}}</view><view class="listcard-content_des-info-item">{{'下载时间：'+item.down_at+''}}</view></view></block></view></view></view></view>